    <servlet-name>StockoutServlet</servlet-name>
    <servlet-class>com.yf.servlet.StockoutServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>StockoutDet</servlet-name>
    <servlet-class>com.yf.servlet.StockoutDet</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>ColorServlet</servlet-name>
    <url-pattern>/ColorServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProductServlet</servlet-name>
    <url-pattern>/ProductServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QueryProCMS</servlet-name>
    <url-pattern>/QueryProCMS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StockinServlet</servlet-name>
