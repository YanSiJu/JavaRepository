    <url-pattern>/StockindetServlet</url-pattern>
  </servlet-mapping>	
  <servlet-mapping>
    <servlet-name>StockoutServlet</servlet-name>
    <url-pattern>/StockoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StockoutDet</servlet-name>
    <url-pattern>/StockoutDet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
